<h1>出勤社員一覧</h1>

<table class="table table-bordered table-condensed user-table">
    
<tr>
<td><b>社員番号</b></td>
<td><b>名前</b></td>
</tr>

<% @users.each do |user| %>
  <% user.attendances.each do |day| %>
    <% if day.started_at.present? && day.finished_at.nil? %>
      <tr>
        <td><%= user.employee_number %></td>
        <td><%= user.name %></td>
      </tr>
    <% end %>
  <% end %>
<% end %>

</table>